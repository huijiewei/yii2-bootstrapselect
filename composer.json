  "description": "The jQuery plugin that brings select elements into the 21st century with intuitive multiselection, searching, and much more. ",
  "minimum-stability": "stable",
  "license": "MIT",
  "type": "yii2-extension",
  "homepage": "https://github.com/huijiewei/yii2-bootstrapselect.git",
  "keywords": [
    "yii2",
    "extension",
    "bootstrap",
    "select",
    "assets"
  ],
  "authors": [
    {
      "name": "Huijie Wei",
      "email": "huijiewei@gmail.com"
    }
  ],
  "require": {
    "php": "^7.1.3",
    "yiisoft/yii2": "^2.0",
    "npm-asset/bootstrap-select": "^1.13.1"
  },
  "autoload": {
    "psr-4": {
      "huijiewei\\bootstrapselect\\": "src"
    }
  },
  "repositories": {
    "asset": {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  }
